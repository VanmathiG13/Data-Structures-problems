class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i,j,temp[]=new int[2];
        for(i=0;i<nums.length-1;i++)
        {
            for(j=i+1;j<nums.length;j++)
            {
            if(nums[i]+nums[j]==target)
            {
                temp[0]=i;
                temp[1]=j;
                break;
            }
            }
        }
        return temp;
    }
}
